.collect_action
  content ''
  position absolute
  right 10%
  top 0
  width 28px
  height 29px
  z-index 1
  bg('~src/assets/gameicon/cards/game_icon_uncollection2@2x.png', 100%)
  &:active
    opacity .8

.collected.collect_action
  bg('~src/assets/gameicon/cards/game_icon_uncollection1@2x.png', 100%)
    
._bg
  &:active
    opacity .8
  for k, v in  urls
    &.{k}
      bg(v+'@2x.png', sizes[k] || 100% 100%)

  for k, v in  labels
    &.{k}:after
      bg(v+'@2x.png', 100%)
  &:after
    content ''
    position absolute
    left 0
    top 0
    width 60px
    height 60px
  
  
    
    
    
  &._notbg:after
    display none
    